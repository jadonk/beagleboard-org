<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
 <head>
  <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
  <!-- meta HTTP-EQUIV="REFRESH" content="1; url=http://www.beagleboard.org" -->
  <title>BeagleBoard.org - <% response.title %></title>
  <link rel="stylesheet" type="text/css" href="/static/default.css" />
 </head>
 <body>
  <div id="menu">
   <!-- div id="nav"><% Nav.items %></div -->
   <div id="user"><% User.prompt %></div>
  </div>
  <form method="post" action="">
   <input name="q" type="text" />
   <button>Search</button>
  </form>
  <hr />
  <% response.body %><br />
  <div id="footer">
   <table>
    <tr>
     <td>
      <a rel="license" href="http://creativecommons.org/licenses/by-sa/3.0/">
       <img alt="Creative Commons License" style="border-width:0" src="/static/images/88x31.png" />
      </a>
     </td>
     <td>
      <table>
       <form action="http://groups.google.com/group/beagleboard/boxsubscribe">
        <tr>
         <td style="padding-left: 50px;">
          email: <input type="text" name="email">
          <input type="submit" name="sub" value="Subscribe">
         </td>
        </tr>
       </form>
      </table>
     </td>
    </tr>
   </table>
  </div>
  <script type="text/javascript">
   var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
   document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
  </script>
  <script type="text/javascript">
   var pageTracker = _gat._getTracker("UA-3429714-1");
   pageTracker._initData();
   pageTracker._trackPageview();
  </script>
 </body>
</html>

