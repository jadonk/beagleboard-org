    &lt;td style=&quot;background-color:#ffffff; padding:0px&quot;&gt;&lt;li&gt;Run Ubuntu &amp; Android&lt;/li&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
    &lt;td style=&quot;background-color:#ffffff; padding:0px&quot;&gt;&lt;li&gt;uHDMI, uSD, USB,       Ethernet, 5V&lt;/li&gt;&lt;/td&gt;
    &lt;td style=&quot;background-color:#ffffff; padding:0px&quot;&gt;&lt;li&gt;Built-in 2GB storage        with Ångström&lt;/li&gt;&lt;/td&gt;
  &lt;/tr&gt;
  
&lt;/table&gt;&lt;br/&gt;
