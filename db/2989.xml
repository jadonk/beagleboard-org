    <body>&lt;h1&gt;Drafts for blog posts&lt;/h1&gt;
&lt;script type=&quot;text/javascript&quot; charset=&quot;utf8&quot; src=&quot;/static/jquery.dataTables.min.js&quot;&gt;&lt;/script&gt;
&lt;script type=&quot;text/javascript&quot;&gt;
    $(document).ready(function () {
        var mainArticles = $(&apos;#leftContent #articlelist&apos;)
        mainArticles.dataTable({
            &quot;sDom&quot;: &quot;tilp&quot;,
            &quot;bSort&quot;: false,
            &quot;sPaginationType&quot;: &quot;full_numbers&quot;,
            &quot;aoColumns&quot;: [
                {&quot;sType&quot;: &quot;int&quot;, &quot;bVisible&quot;:false},
                {&quot;sType&quot;: &quot;html&quot;}
            ],
            &quot;iDisplayLength&quot;: 10
        });
        mainArticles.find(&apos;th&apos;).hide();
        
        $(&apos;#blogSearch&apos;).keyup(function(e){
            mainArticles.dataTable().fnFilter( $(this).val() );
        });
        
        $(&apos;#rightBar&apos;).show();
        
        
        var sideBarArticles = $(&apos;#rightBar #topList&apos;);
        sideBarArticles.dataTable({
            &quot;aaSortingFixed&quot;: [[1, &apos;desc&apos;]],
            &quot;sDom&quot;: &quot;t&quot;,
            &quot;iDisplayLength&quot;: 5,
            &quot;aoColumns&quot;: [
                { &quot;sType&quot;: &quot;html&quot;, &quot;sTitle&quot;: &quot;Title&quot;, &quot;bSortable&quot;:false },
                { &quot;sType&quot;: &quot;numeric&quot;, &quot;sTitle&quot;: &quot;Views&quot;, &quot;bSortable&quot;:false }
            ]
            
        });
    });
&lt;/script&gt;
