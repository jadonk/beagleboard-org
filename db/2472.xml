    <body>&lt;h1&gt;Community&lt;/h1&gt;
&lt;div id=&quot;hp-c2&quot; style=&quot;background-color: #f1f1f1; padding: 10px; width: 250px; border-radius: 5px 5px 5px 5px; float: right; margin: 0px 10px 10px 20px;&quot;&gt;&lt;h1 style=&quot;margin-top: 0px&quot;&gt;Community Activity&lt;/h1&gt;
&lt;script&gt;
var ne = 0;
function handleResponse(resp) {
    for(item in resp.items) {
        var index = parseInt(ne) + parseInt(item);
        var entry = &apos;&apos; +
            &apos;&lt;p&gt;&lt;strong id=&quot;results_&apos; + index + &apos;&quot;&gt;&apos; + 
            &apos;&lt;a href=&quot;&apos; + 
            resp.items[item].url + &apos;&quot;&gt;&apos; + resp.items[item].title.substr(0,100) + 
            &apos;&lt;/a&gt;&lt;/strong&gt;&lt;br /&gt;&apos; +
            &apos;&lt;a href=&quot;&apos; + resp.items[item].url + &apos;&quot;&gt;...more&lt;/a&gt;&lt;/p&gt;&apos;;
        $(&apos;#plus&apos;).append(entry);
    }
    ne += resp.items.length;
}

function makeRequest() {
    gapi.client.plus.activities.list({
        &apos;userId&apos;: &apos;109587204186717390991&apos;,
        &apos;collection&apos;: &apos;public&apos;, 
        &apos;maxResults&apos;: 4
    }).execute(handleResponse);
    console.log(&quot;Made request to Google&quot;);
}

function onGoogleLoad() {
    gapi.client.setApiKey(&apos;AIzaSyDB9-M-HaJ1GtB8j8EppCLsdpzLwW5Y4IE&apos;);
    gapi.client.load(&apos;plus&apos;, &apos;v1&apos;, makeRequest);
    console.log(&quot;Loaded Google API&quot;);
}
&lt;/script&gt;
&lt;script src=&quot;https://apis.google.com/js/client.js?onload=onGoogleLoad&quot;&gt;&lt;/script&gt;

&lt;div id=&quot;plus&quot;&gt;&lt;/div&gt;&lt;br/&gt;
&lt;a href=&quot;https://plus.google.com/communities/104960311812236799231&quot; class=&quot;external&quot;&gt;More Community Activity&lt;/a&gt;

&lt;/div&gt;
&lt;p&gt;Get questions answered or show off your latest Beagle project in the Beagle communities. Tinkering, hacking, making, developing, or simple interest in going where no one&apos;s gone before with the help of powerful, open, embedded computers is welcome here.&lt;/p&gt;

&lt;img src=&quot;http://www.ti.com/ww/en/beagleboard/icons_blog.gif&quot; alt=&quot;blog&quot; width=&quot;106&quot; height=&quot;125&quot; align=&quot;left&quot;&gt;
&lt;h2&gt;&lt;a href=&quot;/blog&quot;&gt;Blog&lt;/a&gt;&lt;/h2&gt;

&lt;p&gt;Read up on all of the news about the BeagleBoard.org Foundation&apos;s fun activities, noteworthy events and generally cool announcements. Go ahead, dig your paws in! &lt;/p&gt;
&lt;br&gt; &lt;/br&gt;&lt;br&gt; &lt;/br&gt;

&lt;img src=&quot;http://www.ti.com/ww/en/beagleboard/icons_chat.gif&quot; alt=&quot;chat&quot; width=&quot;106&quot; height=&quot;125&quot; align=&quot;left&quot;&gt;
&lt;h2&gt;&lt;a href=&quot;/Community/Live Chat&quot;&gt;Live Chat&lt;/a&gt;&lt;/h2&gt;
&lt;p&gt;Post questions and get answers in real-time, monitor for solutions to more complicated queries or join a mailing list to stay up-to-date on all-purpose Beagle news. &lt;/p&gt;
&lt;br&gt; &lt;/br&gt;&lt;br&gt; &lt;/br&gt;

&lt;img src=&quot;http://www.ti.com/ww/en/beagleboard/icons_forum.gif&quot; alt=&quot;forum&quot; width=&quot;106&quot; height=&quot;125&quot; align=&quot;left&quot;&gt;
&lt;h2&gt;&lt;a href=&quot;/Community/Forums&quot;&gt;Forums&lt;/a&gt;&lt;/h2&gt;
&lt;p&gt;Explore the popular discussion forums to track down solutions to common problems (someone&apos;s probably had your question before) or post a new puzzler for the community&apos;s hive mind to solve! &lt;/p&gt;
&lt;br&gt; &lt;/br&gt;&lt;br&gt; &lt;/br&gt;

&lt;img src=&quot;http://www.ti.com/ww/en/beagleboard/icons_video.gif&quot; alt=&quot;video&quot; width=&quot;106&quot; height=&quot;125&quot; align=&quot;left&quot;&gt;
&lt;h2&gt;&lt;a href=&quot;/Videos&quot;&gt;Videos&lt;/a&gt;&lt;/h2&gt;
&lt;p&gt;Learn to troubleshoot common problems, get new application ideas and explore the miscellaneous (but awesome) community activity on our video page.&lt;/p&gt;
&lt;br&gt; &lt;/br&gt;
