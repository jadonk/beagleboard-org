&lt;h1&gt;Cape Demo: BBB-SS Cape v1.2&lt;/h1&gt;
&lt;h2&gt;by Chris Hemmerly&lt;/h2&gt;
&lt;input type=&quot;checkbox&quot; id=&quot;indoor&quot;&gt;Indoor&lt;/input&gt;&lt;br /&gt;
&lt;input type=&quot;checkbox&quot; id=&quot;outdoor&quot;&gt;Outdoor&lt;/input&gt;
&lt;h2&gt;Code&lt;/h2&gt;
&lt;script&gt;
    onbonescriptinit = function() {
        demoRun(&apos;code&apos;);    
    }    
&lt;/script&gt;
&lt;pre id=&apos;code&apos; class=&apos;use-editor&apos; style=&apos;height:240px;&apos;&gt;
var b = require(&apos;bonescript&apos;);
var indoor = &apos;P8_11&apos;;
var outdoor = &apos;P8_16&apos;;
b.pinMode(indoor, b.OUTPUT);
b.pinMode(outdoor, b.OUTPUT);

$(&apos;#indoor&apos;).change(function () {
    b.digitalWrite(indoor, $(this).is(&quot;:checked&quot;)?1:0);
});

$(&apos;#outdoor&apos;).change(function () {
    b.digitalWrite(outdoor, $(this).is(&quot;:checked&quot;)?1:0);
});
&lt;/pre&gt;
&lt;div readonly id=&apos;console-output&apos; style=&apos;height:80px;&apos;&gt;&lt;/div&gt;
&lt;h2&gt;Build and execute instructions&lt;/h2&gt;
&lt;p&gt;Need instructions here on building the BBB-SS Cape, connecting it and running
    the application above.
&lt;/p&gt;
